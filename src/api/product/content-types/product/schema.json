{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "variations": {
      "type": "component",
      "repeatable": true,
      "component": "variation.variations"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "options": {
      "type": "component",
      "repeatable": true,
      "component": "option.options"
    },
    "startingAt": {
      "type": "decimal"
    },
    "estimatedProductionTimeInDays": {
      "type": "integer"
    },
    "profitMargin": {
      "type": "integer"
    },
    "supplier": {
      "type": "string"
    },
    "supplier_code": {
      "type": "string"
    }
  }
}
